{
	"name": "kasimi/pruneusers",
	"type": "phpbb-extension",
	"description": "This extension deletes user accounts whose last activity was before an adjustable amount of time. Founders, inactive accounts and bots are ignored.",
	"homepage": "https://github.com/kasimi/phpbb-ext-pruneusers",
	"version": "1.0.2",
	"time": "2016-11-11",
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "kasimi",
			"email": "mail@kasimi.net",
			"homepage": "https://kasimi.net",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"phpbb/phpbb": ">=3.1.0-b1"
	},
	"extra": {
		"display-name": "Prune Users",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.0-b1"
		}
	}
}
